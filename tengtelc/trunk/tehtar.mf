  draw_dot(0)(dotsize2);
  draw_dot(1)(dotsize2);
  setap("dap",(0,y0+y0+apy));
  setap("cap",(0,y0));
  setap("uap",(0,-apy));
  setap("luap",(x0-0.4pt,-apy+0.1pt));
  setap("ruap",(x1+0.4pt,-apy+0.1pt));
