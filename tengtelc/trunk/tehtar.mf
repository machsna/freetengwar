  y2=0.7[y3,y1];
  y3=-desc;
  x1=0.25[x3,x2];
  x2-x3=0.6pt;
  0=0.55[x3,x2];
  p1=z1{curl 4}..z2{down}..{-dir a3}z3;
  ap1=ang(a1..a2..a3);
  draw_stroke(p1,ap1);
  setap("uap",(0,-apy));
  setap("dap",(0,-3pt));
endchr;
