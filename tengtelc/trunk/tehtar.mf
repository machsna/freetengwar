  z1=(0.5[x0,x2],tex+1pt);
  y0=y2=y1+0.6pt; x2-x1=wt; 0=0.43[x0,x2];
  p1=z0..z1{right}..z2{dir a2};
  ap1=ang(a0..a1..a2);
  draw_stroke(p1,ap1);
  setap("dap",(0,tex+0.5pt));
  setap("ldap",(-1/2wt,tex+0.5pt));
  setap("rdap",(1/2wt,tex+0.5pt));
  setap("uap",(0,tex+1.5pt));
enddef;
