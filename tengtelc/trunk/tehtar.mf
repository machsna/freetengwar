def define_overbar(expr l)=
  x4-x1=l; 1/2[x1,x4]=0;
  x2=0.2[x1,x4]; x3=0.8[x1,x4];
  y1=y4+0.025pt=0.55pt;
  y1-y2=y4-y3=0.1pt;
  p1=z1..z2{dir-2}..{dir2}z3..z4;
  ap1=ang(36--33--35--37);
  penlabels(range 1 thru 4);
enddef;
