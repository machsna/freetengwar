  1/2[x0,x1]=0; x1-x0=width;
  draw_dot(0)(dotsize2);
  draw_dot(1)(dotsize2);
  setap("dap",(0,tex+apy));
  setap("ldap",(x0-0.5pt,tex+apy-0.1pt));
  setap("rdap",(x1+0.5pt,tex+apy-0.1pt));
  setap("uap",(0,y0+(y0-tex)-apy));
