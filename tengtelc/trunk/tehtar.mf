def draw_dblunderstroke=
  a0=40; use(a0); a1=-40;
  z0=(0,-1.4pt);
  z0-z1=z4-z0;
  z0-z2=z3-z0;
  z2-z1=z4-z3=1.9pt*dir (a0-7);
  z3-z1=0.9pt*dir a1;
  draw z1..z2;
  draw z3..z4;
  setap("uap",(0,-apy));
  setap("luap",(-1pt,-apy));
  setap("ruap",(1pt,-apy));
  setap("cap",(0,y0-0.5pt));
  setap("dap",(0,y0-1pt));
enddef;
beginchr(dblunderstroke&".norm",0);
  draw_dblunderstroke;
endchr;
combinechr(dblunderstroke,dottedcircle,"dap",dblunderstroke&".norm","uap");

def draw_engytehta(expr wt) =
